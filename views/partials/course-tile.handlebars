{{#each course}}
<div class="card-style flex-course-col m-1 courses">
    {{!-- <img src="{{courseImage}}" class="card-img-top" alt="..." style="border-radius: 10px 10px 0 0;"> --}}
    <div class="card-body" id="course-section">
        <a href="/courses/{{id}}"><h5 class="card-title">{{title}}</h5></a>
        <p class="card-text">{{courseDescription}}</p>
        <a href="/update-course/{{id}}" class="btn btn-outline-info btn-block"><i class="fas fa-pen-square"></i> Update Course</a>
        <button class="btn btn-danger btn-block delete-button" data-course="{{id}} "><i class="fas fa-trash"></i> Delete Course</button>
    </div>
</div>
{{/each}}